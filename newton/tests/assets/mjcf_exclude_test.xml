<!-- Test file for <exclude> tag parsing -->
<mujoco>
  <worldbody>
    <!-- Body 1 with two geoms -->
    <body name="body1" pos="0 0 1">
      <freejoint/>
      <geom name="body1_geom1" type="box" size="0.1 0.1 0.1" pos="0 0 0"/>
      <geom name="body1_geom2" type="sphere" size="0.05" pos="0.1 0 0"/>
    </body>

    <!-- Body 2 with two geoms -->
    <body name="body2" pos="0 0 2">
      <freejoint/>
      <geom name="body2_geom1" type="box" size="0.1 0.1 0.1" pos="0 0 0"/>
      <geom name="body2_geom2" type="sphere" size="0.05" pos="0.1 0 0"/>
    </body>

    <!-- Body 3 with one geom (should still collide with body1 and body2) -->
    <body name="body3" pos="0 0 3">
      <freejoint/>
      <geom name="body3_geom1" type="box" size="0.1 0.1 0.1" pos="0 0 0"/>
    </body>
  </worldbody>

  <contact>
    <!-- Exclude collisions between body1 and body2 -->
    <exclude body1="body1" body2="body2"/>
  </contact>
</mujoco>
